<%- include("./partials/header")%>
    <body>
        <%- include("./partials/navbar")%>
        <br>
        <div class="container">
            <h1>Perguntas</h1>
            <a class="btn btn-dark" href="perguntar">Perguntar</a>
            <hr>
            <% perguntas.forEach(pergunta=>{ %>
                <div class="card">
                    <div class="card-body">
                        <h3><%= pergunta.titulo %></h3>
                    </div>
                    <div class="card-footer">
                        <a class="btn btn-dark" href="pergunta/<%= pergunta.id %>">responder</a>
                    </div>
                </div>
                <br>
            <% }) %>
        </div>
    </body>
<%- include("./partials/footer")%>